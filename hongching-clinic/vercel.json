{
  "crons": [
    {
      "path": "/api/cron/reminders",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/followup",
      "schedule": "0 2 * * *"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
